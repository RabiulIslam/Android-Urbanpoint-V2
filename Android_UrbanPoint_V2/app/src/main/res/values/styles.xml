<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBarOverlay">true</item>
         </style>

    <style name="TransparentProgressDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>


    <declare-styleable name="HoloCircleSeekBar">
        <attr name="wheel_size" format="integer" />
        <attr name="pointer_size" format="dimension" />
        <attr name="max" format="integer"/>
        <attr name="show_text" format="boolean"/>
        <attr name="start_angle" format="integer"/>
        <attr name="end_angle" format="integer"/>
        <attr name="text_size" format="dimension"/>
        <attr name="init_position" format="integer"/>
        <attr name="wheel_active_color" format="string"/>
        <attr name="wheel_unactive_color" format="string"/>
        <attr name="pointer_color" format="string"/>
        <attr name="pointer_halo_color" format="string"/>
        <attr name="text_color" format="string"/>
    </declare-styleable>


    <style name="CustomIOSTransparentProgressDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="view_20">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">normal</item>
        <!--<inamename="android:textSize">@dimen/sp16</item>-->
        <item name="android:textSize">@dimen/fontsize_normal</item>
        <item name="android:layout_height">@dimen/dp30</item>
    </style>


    <style name="change_pin_view_20" parent="view_20">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_height">@dimen/dp30</item>
    </style>

    <style name="contact_us_view_20" parent="view_20">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_height">@dimen/dp30</item>
    </style>

   <!-- <stnamename="padding_left_right_small">
        <inamename="android:paddingRight">@dimen/four</item>
        <inamename="android:paddingLeft">@dimen/four</item>
    </style>-->

    <style name="button_style">
        <!--<inamename="fontPath">fonts/openSans_lightItalic.ttf</item>-->
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">@dimen/sp18</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:textAllCaps">true</item>
    </style>


    <!-- Base application theme. -->


    <style name="spinnerItemStyle" parent="@android:style/Widget.Holo.DropDownItem.Spinner">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/black_background_border</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp40</item>
    </style>
    <style name="button_large_style" parent="button_style">
        <item name="android:layout_width">@dimen/dp175</item>
        <item name="android:layout_height">@dimen/dp50</item>
    </style>

</resources>

